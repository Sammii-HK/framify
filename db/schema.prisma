// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Template {
  id        String   @id @default(cuid())
  userId    String
  title     String
  prompt    String
  style     String
  code      String   @db.Text
  framerUrl String?
  createdAt DateTime @default(now())
}

